{
	"name": "mc-term",
	"description": "Play Minecraft and configure bots from the terminal",
	"version": "6.0.2",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/MC-Terminal/minecraft-terminal/issues"
	},
	"keywords": [
		"nodejs",
		"javascript",
		"bot",
		"minecraft",
		"console",
		"client",
		"terminal",
		"mod",
		"game",
		"rcon",
		"terminal-based",
		"terminal-game",
		"terminal-client",
		"client",
		"mineflayer",
		"cli"
	],
	"engines": {
		"node": ">=16.0.0"
	},
	"author": {
		"name": "Joe.C"
	},
	"type": "commonjs",
	"repository": {
		"type": "git",
		"url": "https://github.com/MC-Terminal/minecraft-terminal.git"
	},
	"publishConfig": {
		"access": "public"
	},
	"bin": {
		"mc-term": "./dist/index.js"
	},
	"scripts": {
		"build": "npm ci; eslint . --fix; tsc; echo",
		"pkg": "npm run build; pkg . --compress Brotli --options='max-semi-space-size=128,max-old-space-size=300'",
		"prepublishOnly": "npm run build"
	},
	"pkg": {
		"scripts": [
			"node_modules/axios/dist/node/axios.cjs",
			"node_modules/vm2/lib/setup-sandbox.js",
			"dist/builtin_plugins/*"
		],
		"assets": [],
		"targets": [
			"node18-linux-x64",
			"node18-win-x64",
			"node18-macos-x64"
		],
		"outputPath": "builds",
		"compress": "Brotli"
	},
	"main": "./dist/index.js",
	"dependencies": {
		"@iarna/toml": "^3.0.0",
		"axios": "^1.3.4",
		"basic-chat-cli": "^0.1.6",
		"easy-ansi": "^1.2.6",
		"merge": "^2.1.1",
		"minecraft-data": "^3.30.0",
		"minecraft-protocol": "^1.41.1",
		"mineflayer": "4.8.1",
		"mineflayer-maps": "^1.1.2",
		"mineflayer-pathfinder": "^2.4.2",
		"pngjs": "^7.0.0",
		"prismarine-viewer": "^1.24.0",
		"proxy-agent": "^5.0.0",
		"socks": "^2.7.1",
		"vec3": "^0.1.8"
	},
	"devDependencies": {
		"@types/node": "^18.15.0",
		"@types/shelljs": "^0.8.11",
		"@typescript-eslint/eslint-plugin": "^5.54.1",
		"@typescript-eslint/parser": "^5.54.1",
		"eslint": "^8.36.0",
		"eslint-config-standard": "^17.0.0",
		"eslint-config-standard-with-typescript": "^34.0.0",
		"eslint-plugin-import": "^2.27.5",
		"eslint-plugin-n": "^15.6.1",
		"eslint-plugin-promise": "^6.1.1",
		"pkg": "^5.8.1",
		"prismarine-block": "^1.16.3",
		"prismarine-chat": "^1.8.0",
		"prismarine-item": "^1.12.1",
		"prismarine-registry": "^1.7.0",
		"prismarine-windows": "^2.6.1",
		"typed-emitter": "^2.1.0",
		"types-package-json": "^2.0.39",
		"typescript": "^4.9.5"
	}
}
